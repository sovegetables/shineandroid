<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextCrumbs">
        <attr name="text_crumb_item_horizontal_margin" format="dimension"/>
        <attr name="text_crumb_item_vertical_margin" format="dimension"/>
        <attr name="text_crumb_item_gravity">
            <enum name="normal" value="0"/>
            <enum name="center" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CrossView">
        <attr name="backgroundColor" format="reference|color"/>
