    <color name="c_label_layout_close_btn_color">#a6a6a6</color>
    <color name="c_label_layout_title_text_color">#888888</color>
    <color name="c_text_crumb_back">#1a000000</color>
    <dimen name="d_crumb_label_stroke">1dp</dimen>
    <color name="c_filter_card_bg_color">#e2f4fc</color>
    <color name="c_filter_card_title_text_color">#1f1f1f</color>
    <color name="c_filter_card_skip_text_color">#888888</color>
    <color name="c_filter_care_skip_tip_title_text_color">#888888</color>
    <color name="c_crumb_item_default_text_color">#888888</color>
    <color name="c_crumb_item_selected_text_color">#ffffff</color>
    <color name="c_filter_card_item_selected_bg_color">#6cc6f0</color>
    <color name="c_filter_card_no_result_text_color">#888888</color>

</resources>